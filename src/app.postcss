/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

/* For Skeleton App Shell */
html, body { @apply h-full overflow-hidden; }

h1 {
  @apply text-6xl
}

h2 {
  @apply text-5xl
}

h3 {
  @apply text-4xl
}

h4 {
  @apply text-3xl
}
h5 {
  @apply text-2xl
}

a {
  @apply text-primary-500
}
